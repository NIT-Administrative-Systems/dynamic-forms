{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fakerphp/faker": "^1.13",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "laravel/vapor-core": "^2.10",
        "laravel/vapor-ui": "^1.0",
        "livewire/livewire": "^2.4",
        "northwestern-sysdev/laravel-soa": "^7.0",
        "northwestern-sysdev/northwestern-laravel-ui": "^1.4",
        "sentry/sentry-laravel": "^2.4",
        "spatie/laravel-permission": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "facade/ignition": "^2.5",
        "friendsofphp/php-cs-fixer": "^2.18",
        "laravel/sail": "^1.0.1",
        "laravel/vapor-cli": "^1.12",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Northwestern\\SysDev\\DynamicForms\\": "dynamic-forms/src",
            "Northwestern\\SysDev\\DirectoryLookupComponent\\": "directory-lookup-component/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Northwestern\\SysDev\\DynamicForms\\Tests\\": "dynamic-forms/tests",
            "Northwestern\\SysDev\\DirectoryLookupComponent\\Tests\\": "directory-lookup-component/tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    }
}
